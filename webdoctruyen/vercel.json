{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/books.html\\?story=(.*)",
      "dest": "/src/main/resources/templates/books.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/src/main/resources/css/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main/resources/templates/$1"
    }
  ]
}
