{
  "routes": [
    { "src": "/", "dest": "/src/main/resources/templates/index.html" },
    { "src": "/books.html\\?story=(.*)", "dest": "/src/main/resources/templates/$1" },
    { "src": "/css/(.*)", "dest": "/src/main/resources/css/$1" },
    { "src": "/function/(.*)", "dest": "/src/main/resources/function/$1" },
    { "src": "/data/(.*)", "dest": "/src/main/resources/data/$1" }
  ]
}


