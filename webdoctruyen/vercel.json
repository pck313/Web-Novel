{
  "rewrites": [
    { "source": "/", "destination": "/src/main/resources/templates/index.html" }
  ],
  "builds": [
    { "src": "src/main/resources/templates/index.html", "use": "@vercel/static" },
    { "src": "src/main/resources/css/index.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/src/main/resources/css/$1" }
  ]
}
